{
  "name": "streamhub-timeline",
  "description": "A streamHub-backbone plugin that uses a stream of content items to power a scoreboard.",
  "author": {
    "name": "Gene Hallman"
  },
  "version": "0.0.1",
  "devDependencies": {
    "bower": "*",
    "jasmine-node": "*",
    "jsdoc3": "git://github.com/jsdoc3/jsdoc.git#v3.1.0",
    "http-server": "*"
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install",
    "doc": "rm -rf docs && ./node_modules/jsdoc/jsdoc -c jsdoc.conf.json",
    "test": "open tests/index.html",
    "start": "./node_modules/http-server/bin/http-server"
  },
  "streamhub": {
    "sampleImages": [
      "http://superbowl.livefyre.com/images/scoreboard-screenshot.png",
      "https://drive.google.com/uc?id=0BwAX440-rUypanZNSnpuU0NRRWs",
      "https://drive.google.com/uc?id=0BwAX440-rUypaXlmWi1QdC1mODA"
    ]
  }
}
